generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  schemas  = ["auth", "public"]
}

model Profile {
  id              String                  @id @db.Uuid
  email           String?                 @unique
  displayName     String?                 @map("display_name")
  studentNumber   String?                 @map("student_number")
  role            UserRole                @default(STUDENT)
  createdAt       DateTime                @default(now()) @map("created_at")
  updatedAt       DateTime                @updatedAt @map("updated_at")
  auditLogs       AuditLog[]              @relation("ActorAuditLogs")
  sessions        CpxSession[]
  feedbacks       Feedback[]
  users           users?                  @relation(fields: [id], references: [id], onDelete: Cascade)
  scores          Score[]
  reviews         StudentIdVerification[] @relation("ReviewerVerifications")
  idVerifications   StudentIdVerification[] @relation("UserVerifications")
  transcripts       Transcript[]
  uploads           Upload[]
  createdScenarios  Scenario[]              @relation("ScenarioCreator")
  inquiries          Inquiry[]               @relation("UserInquiries")
  answeredInquiries  Inquiry[]               @relation("AnswererInquiries")

  @@map("profiles")
  @@schema("public")
}

model Case {
  id             String       @id @default(uuid()) @db.Uuid
  name           String
  description    String?
  createdAt      DateTime     @default(now()) @map("created_at")
  updatedAt      DateTime     @updatedAt @map("updated_at")
  scenarioJson   Json?        @map("scenario_json")
  checklistJson  Json?        @map("checklist_json")
  chiefComplaint String?      @map("chief_complaint")
  status         CaseStatus?  @default(DRAFT)
  solutionText   String?      @map("solution_text")
  diagnosis      String?      @map("diagnosis")
  sessions       CpxSession[]
  uploads        Upload[]

  @@index([chiefComplaint])
  @@index([status])
  @@map("cases")
  @@schema("public")
}

model CpxSession {
  id          String        @id @default(uuid()) @db.Uuid
  userId      String        @map("user_id") @db.Uuid
  caseId      String?       @map("case_id") @db.Uuid
  origin      CpxOrigin     @default(VP)
  status      SessionStatus @default(IN_PROGRESS)
  startedAt   DateTime      @default(now()) @map("started_at")
  endedAt     DateTime?     @map("ended_at")
  createdAt   DateTime      @default(now()) @map("created_at")
  updatedAt   DateTime      @updatedAt @map("updated_at")
  case        Case?         @relation(fields: [caseId], references: [id])
  user        Profile       @relation(fields: [userId], references: [id])
  feedbacks   Feedback[]
  scores      Score[]
  transcripts Transcript[]
  uploads     Upload[]

  @@index([userId])
  @@index([caseId])
  @@map("cpx_sessions")
  @@schema("public")
}

model Upload {
  id          String      @id @default(uuid()) @db.Uuid
  userId      String      @map("user_id") @db.Uuid
  sessionId   String?     @map("session_id") @db.Uuid
  caseId      String?     @map("case_id") @db.Uuid
  origin      CpxOrigin?  @map("origin")
  type        UploadType
  s3Key       String      @map("s3_key")
  fileName    String?     @map("file_name")
  contentType String?     @map("content_type")
  sizeBytes   Int?        @map("size_bytes")
  createdAt   DateTime    @default(now()) @map("created_at")
  case        Case?       @relation(fields: [caseId], references: [id])
  session     CpxSession? @relation(fields: [sessionId], references: [id])
  user        Profile     @relation(fields: [userId], references: [id])

  @@index([userId])
  @@index([sessionId])
  @@index([caseId])
  @@map("uploads")
  @@schema("public")
}

model Transcript {
  id          String           @id @default(uuid()) @db.Uuid
  userId      String           @map("user_id") @db.Uuid
  sessionId   String?          @map("session_id") @db.Uuid
  source      TranscriptSource @default(UPLOAD)
  origin      CpxOrigin?       @map("origin")
  s3Key       String           @map("s3_key")
  language    String?          @map("language")
  durationSec Int?             @map("duration_sec")
  textExcerpt String?          @map("text_excerpt")
  createdAt   DateTime         @default(now()) @map("created_at")
  updatedAt   DateTime         @updatedAt @map("updated_at")
  sizeBytes   Int?             @map("size_bytes")
  textLength  Int?             @map("text_length")
  tokenCount  Int?             @map("token_count")
  session     CpxSession?      @relation(fields: [sessionId], references: [id])
  user        Profile          @relation(fields: [userId], references: [id])

  @@index([userId])
  @@index([sessionId])
  @@map("transcripts")
  @@schema("public")
}

model Feedback {
  id         String      @id @default(uuid()) @db.Uuid
  userId     String      @map("user_id") @db.Uuid
  sessionId  String?     @map("session_id") @db.Uuid
  origin     CpxOrigin?  @map("origin")
  s3Key      String      @map("s3_key")
  model      String?     @map("model")
  createdAt  DateTime    @default(now()) @map("created_at")
  sizeBytes  Int?        @map("size_bytes")
  textLength Int?        @map("text_length")
  tokenCount Int?        @map("token_count")
  session    CpxSession? @relation(fields: [sessionId], references: [id])
  user       Profile     @relation(fields: [userId], references: [id])

  @@index([userId])
  @@index([sessionId])
  @@map("feedback")
  @@schema("public")
}

model Score {
  id         String      @id @default(uuid()) @db.Uuid
  userId     String      @map("user_id") @db.Uuid
  sessionId  String?     @map("session_id") @db.Uuid
  origin     CpxOrigin?  @map("origin")
  s3Key      String      @map("s3_key")
  total      Float?      @map("total")
  dataJson   Json?       @map("data_json")
  createdAt  DateTime    @default(now()) @map("created_at")
  sizeBytes  Int?        @map("size_bytes")
  textLength Int?        @map("text_length")
  tokenCount Int?        @map("token_count")
  session    CpxSession? @relation(fields: [sessionId], references: [id])
  user       Profile     @relation(fields: [userId], references: [id])

  @@index([userId])
  @@index([sessionId])
  @@map("scores")
  @@schema("public")
}

model StudentIdVerification {
  id           String             @id @default(uuid()) @db.Uuid
  userId       String             @map("user_id") @db.Uuid
  s3Key        String             @map("s3_key")
  status       VerificationStatus @default(PENDING)
  submittedAt  DateTime           @default(now()) @map("submitted_at")
  reviewedBy   String?            @map("reviewed_by") @db.Uuid
  reviewedAt   DateTime?          @map("reviewed_at")
  rejectReason String?            @map("reject_reason")
  reviewer     Profile?           @relation("ReviewerVerifications", fields: [reviewedBy], references: [id])
  user         Profile            @relation("UserVerifications", fields: [userId], references: [id])

  @@index([userId])
  @@index([status])
  @@map("student_id_verifications")
  @@schema("public")
}

model AuditLog {
  id         String   @id @default(uuid()) @db.Uuid
  actorId    String?  @map("actor_id") @db.Uuid
  action     String
  targetType String?  @map("target_type")
  targetId   String?  @map("target_id")
  metadata   Json?
  ipAddress  String?  @map("ip_address")
  userAgent  String?  @map("user_agent")
  createdAt  DateTime @default(now()) @map("created_at")
  actor      Profile? @relation("ActorAuditLogs", fields: [actorId], references: [id])

  @@index([actorId])
  @@index([createdAt])
  @@map("audit_logs")
  @@schema("public")
}

model EvidenceChecklist {
  id             String   @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  chiefComplaint String   @map("chief_complaint")
  version        String
  checklistJson  Json     @map("checklist_json")
  createdAt      DateTime @default(now()) @map("created_at") @db.Timestamptz(6)

  @@unique([chiefComplaint, version])
  @@index([chiefComplaint], map: "idx_evidence_checklists_chief_complaint")
  @@map("evidence_checklists")
  @@schema("public")
}

model Scenario {
  id                       String         @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  chiefComplaint           String         @map("chief_complaint")
  caseName                 String         @map("case_name")
  previousScenarioId       String?        @map("previous_scenario_id") @db.Uuid
  versionNumber            Float          @default(1) @map("version_number")
  status                   ScenarioStatus @default(DRAFT)
  publishedAt              DateTime?      @map("published_at") @db.Timestamptz(6)
  scenarioContent          Json?          @map("scenario_content")
  checklistSourceVersionId String?        @map("checklist_source_version_id") @db.Uuid
  checklistItemsSnapshot   Json?          @map("checklist_items_snapshot")
  checklistIncludedMap     Json?          @map("checklist_included_map")
  checklistConfirmedAt     DateTime?      @map("checklist_confirmed_at") @db.Timestamptz(6)
  commentaryContent        Json?          @map("commentary_content")
  commentaryUpdatedAt      DateTime?      @map("commentary_updated_at") @db.Timestamptz(6)
  rolePromptSnapshot       String?        @map("role_prompt_snapshot")
  commentaryPromptSnapshot String?        @map("commentary_prompt_snapshot")
  promptSourceVersionId    String?        @map("prompt_source_version_id") @db.Uuid
  createdById              String?        @map("created_by_id") @db.Uuid
  createdAt                DateTime       @default(now()) @map("created_at") @db.Timestamptz(6)
  updatedAt                DateTime       @default(now()) @updatedAt @map("updated_at") @db.Timestamptz(6)
  activeImageId            String?        @map("active_image_id") @db.Uuid
  createdBy                Profile?       @relation("ScenarioCreator", fields: [createdById], references: [id], onDelete: SetNull, onUpdate: NoAction)
  patientImages            PatientImage[]
  previousScenario         Scenario?      @relation("ScenarioVersions", fields: [previousScenarioId], references: [id], onDelete: NoAction, onUpdate: NoAction)
  nextVersions             Scenario[]     @relation("ScenarioVersions")

  @@unique([chiefComplaint, caseName, versionNumber])
  @@index([chiefComplaint], map: "idx_scenarios_chief_complaint")
  @@index([chiefComplaint, caseName], map: "idx_scenarios_chief_complaint_case_name")
  @@index([status], map: "idx_scenarios_status")
  @@map("scenarios")
  @@schema("public")
}

model PatientImage {
  id             String    @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  scenarioId     String?   @map("scenario_id") @db.Uuid
  sex            String    @db.VarChar
  age            Int
  chiefComplaint String    @map("chief_complaint") @db.VarChar
  s3Key          String    @map("s3_key") @db.VarChar
  prompt         String?
  model          String?   @default("dall-e-3") @db.VarChar
  sizeBytes      Int?      @map("size_bytes")
  createdAt      DateTime  @default(now()) @map("created_at") @db.Timestamptz(6)
  scenario       Scenario? @relation(fields: [scenarioId], references: [id], onDelete: NoAction, onUpdate: NoAction)

  @@index([chiefComplaint, sex, age], map: "idx_patient_images_chief_complaint_sex_age")
  @@index([scenarioId], map: "idx_patient_images_scenario_id")
  @@map("patient_images")
  @@schema("public")
}

model PromptVersion {
  id        String     @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  type      PromptType
  version   String     @db.VarChar
  content   String
  createdAt DateTime   @default(now()) @map("created_at") @db.Timestamptz(6)

  @@unique([type, version])
  @@index([type], map: "idx_prompt_versions_type")
  @@map("prompt_versions")
  @@schema("public")
}

model ChiefComplaintPrompt {
  id             String       @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  chiefComplaint String       @map("chief_complaint")
  type           CCPromptType @map("type")
  version        String       @db.VarChar
  content        String
  createdAt      DateTime     @default(now()) @map("created_at") @db.Timestamptz(6)

  @@unique([chiefComplaint, type, version])
  @@index([chiefComplaint, type], map: "idx_cc_prompts_chief_complaint_type")
  @@map("chief_complaint_prompts")
  @@schema("public")
}

